APP_NAME=Voltikka
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=Europe/Helsinki
APP_URL=http://localhost

APP_LOCALE=fi
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=fi_FI

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# PostgreSQL on Google Cloud SQL
DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5432
DB_DATABASE=voltikka
DB_USERNAME=postgres
DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# ENTSO-E Transparency Platform API (spot prices)
# Get your key at https://transparency.entsoe.eu/
ENTSOE_API_KEY=

# Digitransit API (geocoding for solar calculator)
# Get your key at https://digitransit.fi/en/developers/api-registration/
DIGITRANSIT_API_KEY=

# Anthropic API (for social media text generation)
# Get your key at https://console.anthropic.com/
ANTHROPIC_API_KEY=

# OpenRouter API (for LLM access - used for social media text generation)
# Get your key at https://openrouter.ai/
OPENROUTER_API_KEY=
OPENROUTER_MODEL=anthropic/claude-sonnet-4

# PostFast API (for social media scheduling to TikTok, Instagram, YouTube, X)
# Get your API key at https://postfa.st/
POSTFAST_API_KEY=

# Remotion video rendering paths (defaults work in Docker)
# REMOTION_PATH=/app/remotion
# REMOTION_OUTPUT_DIR=/app/storage/app/videos
